\begin{abstract}
This little document collects different algorithms from the Google or-tools library.
\end{abstract}

\setcounter{tocdepth}{2}
\tableofcontents

\section{Main search algorithms}

\subsection{Simplified version}

\begin{function}[H]
\SetLine
%\SetKwInOut{Input}{input}\SetKwInOut{Output}{output}
\KwIn{\emph{db}: a constant pointer to a \code{DecisionBuilder}}
\KwIn{\emph{monitors}: a constant reference to a constant \code{std::vector<SearchMonitor*>}}

\ForEach{Task $i$}{
\ForEach{Processor $j$}{
Calculate EW($i$,$j$)\;
\If{EW($i$,$j$) $\le$ EW($a$,$b$)}{
$a$ = $i$\;
$b$ = $j$\;
}
}
}
\While{Unscheduled tasks remaining}{
\ForEach{Processor $j$}{
Calculate FT($a$,$j$)\;
\If{FT($a$,$j$) $\le$ FT($a$,$c$)}{
$c$ = $j$\;
}
}
Schedule(Task $a$ on Processor $c$) \;
}
\caption{NewSearch(db, monitors)}
\label{alg:mine}
\end{function}


