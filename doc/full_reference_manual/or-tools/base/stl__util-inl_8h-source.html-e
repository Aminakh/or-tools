<a name="l00085"></a>00085 <span class="comment">// STLDeleteElements() deletes all the elements in an STL container and clears</span>
<a name="l00086"></a>00086 <span class="comment">// the container.  This function is suitable for use with a vector, set,</span>
<a name="l00087"></a>00087 <span class="comment">// hash_set, or any other STL container which defines sensible begin(), end(),</span>
<a name="l00088"></a>00088 <span class="comment">// and clear() methods.</span>
<a name="l00089"></a>00089 <span class="comment">//</span>
<a name="l00090"></a>00090 <span class="comment">// If container is NULL, this function is a no-op.</span>
<a name="l00091"></a>00091 <span class="comment">//</span>
<a name="l00092"></a>00092 <span class="comment">// As an alternative to calling STLDeleteElements() directly, consider</span>
<a name="l00093"></a>00093 <span class="comment">// ElementDeleter (defined below), which ensures that your container's elements</span>
<a name="l00094"></a>00094 <span class="comment">// are deleted when the ElementDeleter goes out of scope.</span>
<a name="l00095"></a>00095 <span class="keyword">template</span> &lt;<span class="keyword">class</span> T&gt;
<a name="l00096"></a><a class="code" href="namespaceoperations__research.html#02adac4db026e3c0f1cd45a307472888">00096</a> <span class="keywordtype">void</span> <a class="code" href="namespaceoperations__research.html#02adac4db026e3c0f1cd45a307472888" title="STLDeleteElements() deletes all the elements in an STL container and clears the container...">STLDeleteElements</a>(T *container) {
<a name="l00097"></a>00097   <span class="keywordflow">if</span> (!container) <span class="keywordflow">return</span>;
<a name="l00098"></a>00098   <a class="code" href="namespaceoperations__research.html#cacb98b70ed600596ef9db06c72773d4" title="STLDeleteContainerPointers() For a range within a container of pointers, calls delete...">STLDeleteContainerPointers</a>(container-&gt;begin(), container-&gt;end());
<a name="l00099"></a>00099   container-&gt;clear();
<a name="l00100"></a>00100 }
<a name="l00101"></a>00101 
<a name="l00102"></a>00102 <span class="comment">// Given an STL container consisting of (key, value) pairs, STLDeleteValues</span>
<a name="l00103"></a>00103 <span class="comment">// deletes all the "value" components and clears the container.  Does nothing</span>
<a name="l00104"></a>00104 <span class="comment">// in the case it's given a NULL pointer.</span>
<a name="l00105"></a>00105 
<a name="l00106"></a>00106 <span class="keyword">template</span> &lt;<span class="keyword">class</span> T&gt;
<a name="l00107"></a><a class="code" href="namespaceoperations__research.html#cc1208da53b8c9966104889d06555424">00107</a> <span class="keywordtype">void</span> <a class="code" href="namespaceoperations__research.html#cc1208da53b8c9966104889d06555424" title="Given an STL container consisting of (key, value) pairs, STLDeleteValues deletes...">STLDeleteValues</a>(T *v) {
<a name="l00108"></a>00108   <span class="keywordflow">if</span> (!v) <span class="keywordflow">return</span>;
<a name="l00109"></a>00109   <span class="keywordflow">for</span> (<span class="keyword">typename</span> T::iterator i = v-&gt;begin(); i != v-&gt;end(); ++i) {
<a name="l00110"></a>00110     <span class="keyword">delete</span> i-&gt;second;
<a name="l00111"></a>00111   }
<a name="l00112"></a>00112   v-&gt;clear();
<a name="l00113"></a>00113 }
<a name="l00114"></a>00114 
<a name="l00115"></a>00115 }  <span class="comment">// namespace operations_research</span>
