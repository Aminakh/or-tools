<a name="l00085"></a>00085   <a class="code" href="namespaceoperations__research.html#bf5e2c0c50eea097607546034df2c85d" title="Max Flow.">operations_research::MaxFeasibleFlow</a>();
<a name="l00086"></a>00086   <span class="keywordflow">return</span> 0;
<a name="l00087"></a>00087 }
