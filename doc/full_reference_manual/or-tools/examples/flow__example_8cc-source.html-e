<a name="l00023"></a>00023 <span class="comment">// ----- Min Cost Flow -----</span>
<a name="l00024"></a>00024 
<a name="l00025"></a>00025 <span class="comment">// Test on a 4x4 matrix. Example taken from</span>
<a name="l00026"></a>00026 <span class="comment">// http://www.ee.oulu.fi/~mpa/matreng/eem1_2-1.htm</span>
<a name="l00027"></a><a class="code" href="namespaceoperations__research.html#9c063d59883516b1947c0c9934dc8acf">00027</a> <span class="keywordtype">void</span> <a class="code" href="namespaceoperations__research.html#9c063d59883516b1947c0c9934dc8acf" title="Min Cost Flow.">MinCostFlowOn4x4Matrix</a>() {
<a name="l00028"></a>00028   LOG(INFO) &lt;&lt; <span class="stringliteral">"Min Cost Flow on 4x4 Matrix"</span>;
<a name="l00029"></a>00029   <span class="keyword">const</span> <span class="keywordtype">int</span> kNumSources = 4;
<a name="l00030"></a>00030   <span class="keyword">const</span> <span class="keywordtype">int</span> kNumTargets = 4;
<a name="l00031"></a>00031   <span class="keyword">const</span> CostValue kCost[kNumSources][kNumTargets] = {
<a name="l00032"></a>00032     { 90, 75, 75, 80 },
<a name="l00033"></a>00033     { 35, 85, 55, 65 },
<a name="l00034"></a>00034     { 125, 95, 90, 105 },
<a name="l00035"></a>00035     { 45, 110, 95, 115 }
<a name="l00036"></a>00036   };
<a name="l00037"></a>00037   <span class="keyword">const</span> CostValue kExpectedCost = 275;
<a name="l00038"></a>00038   StarGraph graph(kNumSources + kNumTargets, kNumSources * kNumTargets);
<a name="l00039"></a>00039   MinCostFlow min_cost_flow(graph);
<a name="l00040"></a>00040   <span class="keywordflow">for</span> (NodeIndex source = 1; source &lt;= kNumSources; ++source) {
<a name="l00041"></a>00041     <span class="keywordflow">for</span> (NodeIndex target = 1; target &lt;= kNumTargets; ++target) {
<a name="l00042"></a>00042       ArcIndex arc = graph.AddArc(source, kNumSources + target);
<a name="l00043"></a>00043       min_cost_flow.SetArcUnitCost(arc, kCost[source -  1][target - 1]);
<a name="l00044"></a>00044       min_cost_flow.SetArcCapacity(arc, 1);
<a name="l00045"></a>00045     }
<a name="l00046"></a>00046   }
<a name="l00047"></a>00047   <span class="keywordflow">for</span> (NodeIndex source = 1; source &lt;= kNumSources; ++source) {
<a name="l00048"></a>00048     min_cost_flow.SetNodeSupply(source, 1);
<a name="l00049"></a>00049   }
<a name="l00050"></a>00050   <span class="keywordflow">for</span> (NodeIndex target = 1; target &lt;= kNumTargets; ++target) {
<a name="l00051"></a>00051     min_cost_flow.SetNodeSupply(kNumSources + target, -1);
<a name="l00052"></a>00052   }
<a name="l00053"></a>00053   CostValue total_flow_cost = min_cost_flow.ComputeMinCostFlow();
<a name="l00054"></a>00054   CHECK_EQ(kExpectedCost, total_flow_cost);
<a name="l00055"></a>00055 }
<a name="l00056"></a>00056 
<a name="l00057"></a>00057 <span class="comment">// ----- Max Flow -----</span>
<a name="l00058"></a>00058 
<a name="l00059"></a><a class="code" href="namespaceoperations__research.html#bf5e2c0c50eea097607546034df2c85d">00059</a> <span class="keywordtype">void</span> <a class="code" href="namespaceoperations__research.html#bf5e2c0c50eea097607546034df2c85d" title="Max Flow.">MaxFeasibleFlow</a>() {
<a name="l00060"></a>00060   LOG(INFO) &lt;&lt; <span class="stringliteral">"Max Feasible Flow"</span>;
<a name="l00061"></a>00061   <span class="keyword">const</span> <span class="keywordtype">int</span> kNumNodes = 6;
<a name="l00062"></a>00062   <span class="keyword">const</span> <span class="keywordtype">int</span> kNumArcs = 9;
<a name="l00063"></a>00063   <span class="keyword">const</span> NodeIndex kTail[kNumArcs] = { 1, 1, 1, 1, 2, 3, 4, 4, 5 };
<a name="l00064"></a>00064   <span class="keyword">const</span> NodeIndex kHead[kNumArcs] = { 2, 3, 4, 5, 4, 5, 5, 6, 6 };
<a name="l00065"></a>00065   <span class="keyword">const</span> FlowQuantity <a class="code" href="cvrptw_8cc.html#473a6fb33311094d5da06f6b7527a13b">kCapacity</a>[kNumArcs] = { 5, 8, 5, 3, 4, 5, 6, 6, 4 };
<a name="l00066"></a>00066   <span class="keyword">const</span> FlowQuantity kExpectedFlow[kNumArcs] = { 4, 4, 2, 0, 4, 4, 0, 6, 4 };
<a name="l00067"></a>00067   <span class="keyword">const</span> FlowQuantity kExpectedTotalFlow = 10;
<a name="l00068"></a>00068   StarGraph graph(kNumNodes, kNumArcs);
<a name="l00069"></a>00069   MaxFlow max_flow(graph, 1, kNumNodes);
<a name="l00070"></a>00070   <span class="keywordflow">for</span> (<span class="keywordtype">int</span> i = 0; i &lt; kNumArcs; ++i) {
<a name="l00071"></a>00071     ArcIndex arc = graph.AddArc(kTail[i], kHead[i]);
<a name="l00072"></a>00072     max_flow.SetArcCapacity(arc, kCapacity[i]);
<a name="l00073"></a>00073   }
<a name="l00074"></a>00074   FlowQuantity total_flow = max_flow.ComputeMaxFlow();
<a name="l00075"></a>00075   CHECK_EQ(total_flow, kExpectedTotalFlow);
<a name="l00076"></a>00076   <span class="keywordflow">for</span> (<span class="keywordtype">int</span> i = 0; i &lt; kNumArcs; ++i) {
<a name="l00077"></a>00077     CHECK_EQ(kExpectedFlow[i], max_flow.Flow(i + 1)) &lt;&lt; <span class="stringliteral">" i = "</span> &lt;&lt; i;
<a name="l00078"></a>00078   }
<a name="l00079"></a>00079 }
<a name="l00080"></a>00080 }  <span class="comment">// namespace operations_research</span>
<a name="l00081"></a>00081 
